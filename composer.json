{
  "name": "bektas/cart",
  "description": "A cart",
  "license": "MIT",
  "keywords": [
    "laravel",
    "cart",
    "cart library",
    "eloquent cart"
  ],
  "type": "library",
  "authors": [
    {
      "name": "BektaÅŸ",
      "email": "bektasstar@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5.6"
  },
  "require-dev": {
    "orchestra/testbench": "~3.6.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Bektas\\Cart\\": "src"
    },
    "files": [
      "helpers/cartHelper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Bektas\\Cart\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bektas\\Cart\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
